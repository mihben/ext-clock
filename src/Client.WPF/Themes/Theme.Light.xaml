<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Background" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="Text" Color="Black"></SolidColorBrush>

    <SolidColorBrush x:Key="Section__Border" Color="DarkGray"/>
    
    <SolidColorBrush x:Key="Settings__Menu" Color="Black"/>

    <SolidColorBrush x:Key="Field--Required" Color="LightYellow"/>
</ResourceDictionary>